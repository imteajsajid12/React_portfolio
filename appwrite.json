{
  "projectId": "673dca020027a68a026f",
  "projectName": "demo_react",
  "functions": [],
  "databases": [
    {
      "databaseId": "68b167ac00260b8be1b2",
      "name": "portfolio_database",
      "collections": [
        {
          "collectionId": "portfolio_projects",
          "name": "Projects",
          "permissions": ["read(\"any\")"],
          "documentSecurity": false,
          "attributes": [
            {
              "key": "title",
              "type": "string",
              "size": 255,
              "required": true
            },
            {
              "key": "description",
              "type": "string",
              "size": 1000,
              "required": true
            },
            {
              "key": "technologies",
              "type": "string",
              "size": 500,
              "required": false,
              "array": true
            },
            {
              "key": "image",
              "type": "string",
              "size": 255,
              "required": false
            },
            {
              "key": "demoUrl",
              "type": "url",
              "required": false
            },
            {
              "key": "githubUrl",
              "type": "url",
              "required": false
            },
            {
              "key": "featured",
              "type": "boolean",
              "required": false,
              "default": false
            },
            {
              "key": "order",
              "type": "integer",
              "required": false,
              "default": 0
            }
          ],
          "indexes": [
            {
              "key": "featured_order",
              "type": "key",
              "attributes": ["featured", "order"]
            }
          ]
        }
      ]
    }
  ],
  "storage": [
    {
      "bucketId": "674958ca00169e907ea0",
      "name": "portfolio_files",
      "permissions": ["read(\"any\")"],
      "fileSecurity": false,
      "enabled": true,
      "maximumFileSize": 10485760,
      "allowedFileExtensions": ["jpg", "jpeg", "png", "gif", "svg", "webp", "pdf", "doc", "docx"],
      "compression": "gzip",
      "encryption": false,
      "antivirus": false
    }
  ]
}
